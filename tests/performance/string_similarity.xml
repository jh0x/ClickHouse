<test>
    <query>SELECT avg(levenshteinDistance(randomString(300), randomString(300))) FROM numbers(10000)</query>
    <query>SELECT avg(levenshteinDistanceUTF8(randomStringUTF8(300), randomStringUTF8(300))) FROM numbers(10000)</query>
    
    <query>SELECT avg(levenshteinDistance(randomString(100), randomString(200))) FROM numbers(500000)</query>
    <query>SELECT avg(levenshteinDistanceUTF8(randomStringUTF8(100), randomStringUTF8(200))) FROM numbers(50000)</query>
    
    <query>SELECT avg(levenshteinDistance(randomString(100), randomString(101))) FROM numbers(500000)</query>
    <query>SELECT avg(levenshteinDistanceUTF8(randomStringUTF8(100), randomStringUTF8(101))) FROM numbers(100000)</query>
    
    <query>SELECT avg(levenshteinDistance(randomString(20), randomString(100))) FROM numbers(2000000)</query>
    <query>SELECT avg(levenshteinDistanceUTF8(randomStringUTF8(20), randomStringUTF8(100))) FROM numbers(200000)</query>

    <query>SELECT avg(levenshteinDistance(randomString(20), randomString(60))) FROM numbers(2000000)</query>
    <query>SELECT avg(levenshteinDistanceUTF8(randomStringUTF8(20), randomStringUTF8(60))) FROM numbers(200000)</query>

    <query>SELECT avg(stringJaccardIndex(randomString(100), randomString(101))) FROM numbers(10000000)</query>
    <query>SELECT avg(stringJaccardIndexUTF8(randomStringUTF8(100), randomStringUTF8(101))) FROM numbers(50000)</query>
</test>
